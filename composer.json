{
	"name": "artskills/common",
	"description": "common utils",
	"type": "library",
	"repositories": [
		{
			"type": "vcs",
			"url": "git://github.com/ArtSkills/codeStyle.git"
		}
	],
	"require": {
		"php": "^7.1",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-curl": "*",
		"ext-pdo": "*",
		"cakephp/cakephp": "^3.8",
		"robmorgan/phinx": "^0.6.2",
		"sentry/sentry": "^1.1",
		"jan-swiecki/simple-annotations": "*"
	},
	"require-dev": {
		"ext-runkit": "*",
		"ext-iconv": "*",
		"phpunit/phpunit": "^6",
		"cakephp/debug_kit": "~3.1",
		"artskills/codestyle": "1"
	},
	"autoload": {
		"psr-4": {
			"ArtSkills\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ArtSkills\\Test\\": "tests",
			"TestApp\\": "test-app"
		}
	}
}
